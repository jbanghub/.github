{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/\\.java/"
      ],
      "matchStrings": [
        "//DEPS (?<depName>\\S+):(\\${\\S+:(?<currentValue>[^ }]+)})",
        "//DEPS ((?<depName>\\S+:\\S+):(?<currentValue>[^$]\\S+))(\\@\\w+)?"
      ],
      "datasourceTemplate": "maven"
    },
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/.?jbang-catalog.json/"
      ],
      "matchStrings": [
        "\"(?<depName>\\S+):(\\${\\S+:(?<currentValue>[^ }]+)})\"",
        "\"((?<depName>\\S+:\\S+):(?<currentValue>[^$]\\S+))(\\@\\w+)?\""
      ],
      "datasourceTemplate": "maven"
    }
  ]
}